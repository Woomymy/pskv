{
    "name": "pskv",
    "version": "1.1.0",
    "description": "Simple key-value storage for PostGresSQL",
    "main": "dist/lib.js",
    "types": "dist/lib.d.ts",
    "scripts": {
        "test": "jest --passWithNoTests",
        "prepublishOnly": "tsc",
        "start-build": "tsc && node dist/index.js",
        "build": "tsc",
        "lint": "eslint . -c .eslintrc.cjs",
        "lint-fix": "eslint . -c .eslintrc.cjs --fix",
        "format": "prettier 'src/**/*.ts' --write",
        "gendocs": "typedoc && cp doc/font.ttf docs/assets/font.ttf && echo \"pskv.woomy.be\" > docs/CNAME"
    },
    "author": "Woomymy",
    "license": "MIT",
    "type": "module",
    "devDependencies": {
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.18",
        "@types/pg": "^8.6.6",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.3.1",
        "jest-ts-webcompat-resolver": "^1.0.0",
        "prettier": "^2.8.1",
        "ts-jest": "^29.0.3",
        "typedoc": "^0.23.23",
        "typescript": "^4.9.4",
        "eslint": "^8.31.0"
    },
    "dependencies": {
        "ts-postgres": "^1.3.0"
    }
}
