{
    "name": "pskv",
    "version": "1.0.1",
    "description": "Simple key-value storage for PostGresSQL",
    "main": "dist/lib.js",
    "types": "dist/lib.d.ts",
    "scripts": {
        "test": "jest --passWithNoTests",
        "prepublishOnly": "tsc",
        "start-build": "tsc && node dist/index.js",
        "build": "tsc",
        "lint": "eslint . -c .eslintrc.cjs",
        "lint-fix": "eslint . -c .eslintrc.cjs --fix",
        "format": "prettier 'src/**/*.ts' --write",
        "gendocs": "typedoc && cp doc/font.ttf docs/assets/font.ttf && echo \"pskv.woomy.ovh\" > docs/CNAME"
    },
    "author": "Woomymy",
    "license": "MIT",
    "type": "module",
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/node": "^16.9.2",
        "@types/pg": "^8.6.1",
        "@typescript-eslint/eslint-plugin": "^4.31.2",
        "@typescript-eslint/parser": "^4.31.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.1",
        "jest-ts-webcompat-resolver": "^1.0.0",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.5",
        "typedoc": "^0.22.4",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "eslint": "^7.32.0",
        "pg": "^8.7.1"
    }
}
