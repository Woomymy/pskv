{
    "name": "pskv",
    "version": "1.1.2",
    "description": "Simple key-value storage for PostGresSQL",
    "main": "dist/lib.js",
    "types": "dist/lib.d.ts",
    "scripts": {
        "test": "jest --passWithNoTests",
        "prepublishOnly": "tsc",
        "start-build": "tsc && node dist/index.js",
        "build": "tsc",
        "lint": "eslint . -c .eslintrc.cjs",
        "lint-fix": "eslint . -c .eslintrc.cjs --fix",
        "format": "prettier 'src/**/*.ts' --write",
        "gendocs": "typedoc && cp doc/font.ttf docs/assets/font.ttf && echo \"pskv.woomy.be\" > docs/CNAME"
    },
    "author": "Woomymy",
    "license": "MIT",
    "type": "module",
    "devDependencies": {
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.5",
        "@types/pg": "^8.10.2",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "eslint-config-prettier": "^8.9.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.6.2",
        "jest-ts-webcompat-resolver": "^1.0.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "typedoc": "^0.24.8",
        "typescript": "^5.1.6",
        "eslint": "^8.46.0"
    },
    "dependencies": {
        "ts-postgres": "^1.3.1"
    }
}
