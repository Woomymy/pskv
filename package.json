{
    "name": "pskv",
    "version": "1.0.3",
    "description": "Simple key-value storage for PostGresSQL",
    "main": "dist/lib.js",
    "types": "dist/lib.d.ts",
    "scripts": {
        "test": "jest --passWithNoTests",
        "prepublishOnly": "tsc",
        "start-build": "tsc && node dist/index.js",
        "build": "tsc",
        "lint": "eslint . -c .eslintrc.cjs",
        "lint-fix": "eslint . -c .eslintrc.cjs --fix",
        "format": "prettier 'src/**/*.ts' --write",
        "gendocs": "typedoc && cp doc/font.ttf docs/assets/font.ttf && echo \"pskv.woomy.ovh\" > docs/CNAME"
    },
    "author": "Woomymy",
    "license": "MIT",
    "type": "module",
    "devDependencies": {
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.10",
        "@types/pg": "^8.6.4",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.4.7",
        "jest-ts-webcompat-resolver": "^1.0.0",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "typedoc": "^0.22.11",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "eslint": "^8.7.0",
        "pg": "^8.7.1"
    }
}
